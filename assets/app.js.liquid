  var newReleases = _.get(metaData, 'new_releases', []);
  var availableForPreOrders = _.get(metaData, 'pre_orders', []);
  
  if (!_.isEmpty(newReleases)) {
    $product.find('.new_banner_product').hide();
    if (-1 !== _.indexOf(newReleases, variant.options[colorOptionIndex].toLowerCase())) {
      $product.find('.new_banner_product').show();
    }
  }

  if (!_.isEmpty(availableForPreOrders)) {
    $product.find('.preorder_banner_product').hide();
    if (-1 !== _.indexOf(availableForPreOrders, variant.options[colorOptionIndex].toLowerCase())) {
      $product.find('.preorder_banner_product').show();
    }
  }
});

