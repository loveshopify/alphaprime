var AP = {
  windowWidth: 0,
  scrollTop: function(scrollTop, animate, speed) {
    if (animate && speed) {
      $('html, body').animate({scrollTop: scrollTop}, speed)
      return
    }

    $('html, body').scrollTop(scrollTop)
  },
  ajax: {
    addToCart: function(productId, qty, cb) {
      jQuery.post('/cart/add.js', {
        id: productId,
        quantity: qty
      }).done(function() {
        if (_.isFunction(cb)) {
          cb()
        }
      })
    },
    getCart: function() {
      return new Promise(function(resolve, reject) {
        $.ajax({
          url: '/cart.js',
          dataType: 'json',
          cache: false,
          success: function(cart) {
            resolve(cart)
          },
          error: function(err) {
            reject(err)
          }
        });
      })
    }
  },
  isProductPage: false
}

jQuery(document).ready(function($) {
  AP.$window = $(window)
  AP.windowWidth = AP.$window.width()
  AP.$html = $('html')
  AP.$body = $('body')

  if (AP.$body.hasClass('single-product-page')) {
    AP.isProductPage = true;
  }

  $(window).trigger('AP:ready', [AP]);

  AP.$window.on('resize', _.throttle(function(event) {
    AP.windowWidth = AP.$window.width()
  }, 100))

  // ajax add to cart button
  $(document).on('click', 'a.add_to_cart', function(event) {
    event.preventDefault()
    var $this = $(this)
    var productId = $this.data('productid') || false
    
    if (!productId || $this.prop('disabled')) return
    
    $this.prop('disabled', true)

    AP.ajax.addToCart(productId, 1, function() {
      location.reload()
    })
  })
})

$(window).on('AP:ready', function(event, AP) {
  $.extend(true, $.magnificPopup.defaults, {
    preloader: false,
    alignTop: true,
    fixedContentPos: true,
    fixedBgPos: true,
    closeBtnInside: false,
    callbacks: {
      beforeOpen: function() {
        AP.$html.data('scroll-top', AP.$window.scrollTop());
        AP.$html.addClass('mfp--true');
      },
      close: function() {
        AP.$html.removeClass('mfp--true');
        AP.$window.scrollTop(AP.$html.data('scroll-top'));
      }
    }
  });
})

