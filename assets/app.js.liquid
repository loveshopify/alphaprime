  // Updated 11/09/2018 by Eugene
  initializeQuantityBox: function() {
    $(document).on('change', 'input.quantity', function() {
        var $this = $(this),
            val = parseInt($this.val().trim()),
            _minQty = $this.attr('min') || 1,
            _maxQty = $this.attr('max') || 99;
        
        if (isNaN(val) || val < _minQty) {
          $this.val(_minQty);
          return;
        } else if (val > _maxQty) {
          $this.val(_maxQty);
          return;
        }

        if($this.parents(".cart_item").length) {
          if (val - 1 == 0) {
            $this.closest('.cart_item').addClass('animated fadeOutUp')
          }
        }
    });
    
    $(document).on('click', '.js-change-quantity', function() {
      var $this = $(this),
          $qtyInput = $this.siblings('input'),
          val = parseInt($qtyInput.val().trim());

      if (isNaN(val)) {
        val = 0;
      }

      if($this.data('func') == 'plus') {
        val += 1;
      } else {
        val -= 1;
      }

      $qtyInput.val(Math.max(0, val)).trigger('change');
    });
  },
  unload: function($target) {
    var $slider = $target.find('.products-slider');
    $slider.flickity('destroy');
  }
}

