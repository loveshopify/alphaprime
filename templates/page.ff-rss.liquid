   {% else %}
   <g:title><![CDATA[{{ product.title | strip_html | strip_newlines | escape | replace: '&amp;', 'and' | replace: 'amp;', 'and' | replace: '&#38;', 'and' | replace: "u00c3u00a9", "e" | replace: '&quot;', '' | replace: "u00c3 ", "a" }}]]></g:title>
