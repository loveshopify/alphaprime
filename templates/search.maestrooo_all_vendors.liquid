{%- layout none -%}

{%- capture all_vendors -%}
  [
    {%- for vendor in shop.vendors -%}
      {
        "object": "vendor",
        "title": {{ vendor | json }}
      }{%- unless forloop.last -%},{%- endunless -%}
    {%- endfor -%}
  ]
{%- endcapture -%}

callback_maestrooo_all_vendors_{{ search.terms | md5 }}({
  "object": "list",
  "data": {{ all_vendors }},
  "meta": {
    "total_count": {{ shop.vendors.size }},
    "has_more": false
  }
})