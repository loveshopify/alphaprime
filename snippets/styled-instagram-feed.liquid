</style>
<div id="styled-instagram-feed" style="display: none">
  <div style="text-align: center;font-size: 14px;margin: 8px 0 12px;"><a href="https://alphaprimeusa.com/pages/shopig" class="icon-instagram">Styled On Instagram</a></div>
  <script src="//foursixty.com/media/scripts/fs.slider.v2.5.js" data-feed-id="alpha-prime-apparel_1" data-theme="slider_v2_5" data-connector-filter="27537,29391,29392,29393,29394,30696" data-for-url="true"></script>
  <script type="text/javascript">
    jQuery(document).ready(function($) {
      $(window).on('load', function() {
          $(".fs-has-posts").remove();
          $(".centered-content.fs-slider-next-button").remove();
          $(".centered-content.fs-slider-prev-button").remove();
          $(".centered-content.fs-slider-prev-button").removeClass();
          var slider = $("#styled-instagram-feed .fs-wrapper");

          slider.removeClass("fs-slider_v2_5-feed");
          slider.removeClass("fs-timeline");
          slider.removeClass("fs-wrapper");
          slider.addClass("slick-slider");

          var sliderParent = $(".fs-slider-container.fs-narrow-timeline");
          sliderParent.removeClass("fs-narrow-timeline");
          sliderParent.removeClass("fs-slider-container");
          sliderParent.removeClass("fs-desktop");
          sliderParent.removeClass("fs-mobile");
          sliderParent.removeClass("fs-slider_v2_5");
          sliderParent.addClass("fs-desktop");
        
          onSlickSetup(slider, true);
        
        window.addEventListener("orientationchange", function() {
         onSlickSetup(slider, false);
        }, false);
        window.addEventListener("resize", function() {
           onSlickSetup(slider, true);
        }, false);
      });
    });
    function onSlickSetup(slider, isResize) {
      var styledInstagramFeed = $('#styled-instagram-feed');
      if (styledInstagramFeed.find('.fs-entry-container').length <= 0) {
        return;
      }

      if (slider.hasClass('slick-initialized')) {
        slider.slick('unslick');
      }
      var isPC = window.screen.width >= 768;
      if (!isResize) {
        isPC = !isPC;
      }
      if (isPC) {
        slider.slick({
          dots: false,
          infinite: false,
          slidesToShow: 4,
          slidesToScroll: 1,
          arrows: true,
          vertical: true,
          verticalSwiping: true,
          prevArrow: '<span class="slickarrow icon-up-arrow"></span>',
          nextArrow: '<span class="slickarrow icon-down-arrow"></span>'
        });
      } else {
        slider.slick({
          dots: false,
          infinite: false,
          slidesToShow: 2,
          slidesToScroll: 1,
          arrows: false,
          vertical: false,
          verticalSwiping: false,
        });
      }
      styledInstagramFeed.show();
    }
  </script>
</div>