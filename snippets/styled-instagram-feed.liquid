#styled-instagram-feed div.fs-detail-container .fs-post-info,
#styled-instagram-feed div.fs-wrapper div.fs-has-links::after,
#styled-instagram-feed .fs-text-product,
#styled-instagram-feed .fs-overlink-text {
  font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
  font-style: normal;
  font-weight: normal;
}
#styled-instagram-feed .fs-wrapper div.fs-text-container * {
  color: rgb(255, 255, 255);
}
#styled-instagram-feed .fs-wrapper div.fs-text-container {
  background-color: rgba(0, 0, 0, 0.25);
  margin: 0px;
}
#styled-instagram-feed div.fs-entry-date,
#styled-instagram-feed div.fs-entry-title {
  display: none;
}
#styled-instagram-feed .fs-wrapper div.fs-timeline-entry {
  margin: 1px;
}
  
  .fs-mobile .fs-entry-container, .fs-desktop .fs-entry-container {
    padding-top: 0;
    height: 221px;
    width: 221px;
    float: none;
  }

  #styled-instagram-feed .fs-desktop div.fs-entry-container {
    width: 100% !important;
    padding-top: 25% !important;
  }
  #styled-instagram-feed .fs-mobile div.fs-entry-container {
      height: 130px !important;
      width: 96% !important;
  }
  
  /*mobile*/
  @media all and (max-width: 480px) {
    #styled-instagram-feed .fs-desktop div.fs-entry-container {
      height: 100px;
    }
    #styled-instagram-feed .fs-mobile div.fs-entry-container {
      height: 100px !important;
      width: 96% !important;
    }
  }
  @media all and (max-width: 767px) {
    #styled-instagram-feed .fs-desktop div.fs-entry-container {
      height: 120px;
    }
    #styled-instagram-feed .fs-mobile div.fs-entry-container {
      height: 130px !important;
      width: 96% !important;
    }
  }
  /*pc*/
  @media all and (min-width: 768px) {
    #styled-instagram-feed .fs-desktop div.fs-entry-container {
      height: 140px;
